<h2 class="page-header">Delete Folder</h2>
<form method="POST" action="/folders/<%= name %>/delete">
  <div class="form-row form-row-end btn-row">
    <a class="btn btn-danger" href="/folders/<%= name %>">Cancel</a>
    <button
      class="btn btn-danger"
      type="submit"
      onclick="return confirm('Are you sure?')"
    >
      Delete
    </button>
  </div>
</form>
